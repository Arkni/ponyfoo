.as-chart svg
  overflow visible

.as-axis path,
.as-axis line
  fill none
  shape-rendering crispEdges

.as-axis,
.as-legend
  fill c-black

.as-x path
  display none

.as-legend
  text-transform capitalize

.as-tip
  line-height 1
  margin-left 15px

.as-tip-content
  padding 8px
  text-align left

.as-tip-label
  text-transform capitalize
